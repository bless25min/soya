            left: 50%;
            transform: translateX(-50%);
            top: 0;
            bottom: 0;
            width: 4px;
            background: rgba(255, 255, 255, 0.3);
        }

        .process-step {
            position: relative;
            margin: 3rem 0;
            width: 45%;
        }

        .process-step:nth-child(odd) {
            left: 0;
            text-align: right;
        }

        .process-step:nth-child(even) {
            left: 55%;
            text-align: left;
        }

        .process-content {
            background: rgba(255, 255, 255, 0.1);
            padding: 2rem;
            border-radius: 15px;
            backdrop-filter: blur(10px);
